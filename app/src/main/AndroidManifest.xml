<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teachingassistant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Support.Bean.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Main.LoadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.Main.LoginActivity" />
        <activity android:name=".Activity.Search.SearchMainActivity" />
        <activity
            android:name=".Activity.Search.CourseSearchActivity" />
        <activity
            android:name=".Activity.Search.GradeActivity"
            android:parentActivityName=".Activity.Search.SearchMainActivity" />
        <activity
            android:name=".Activity.Search.ExamActivity"
            android:parentActivityName=".Activity.Search.SearchMainActivity" />
        <activity android:name=".Activity.Search.EmptyRoomActivity" />
        <activity android:name=".Activity.Search.CourseActivity"/>
    </application>

</manifest>